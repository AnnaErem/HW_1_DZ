    "class Rectangle:\n",
    "    def __init__(self, p1, p2):\n",
    "        # p1 и p2 — кортежи (x, y) противоположных углов\n",
    "        self.x1, self.y1 = p1\n",
    "        self.x2, self.y2 = p2 # self - это ссылка на текущий объект (экземпляр класса) необходимо прописывать чтобы взаимодействовать с переммыными в классе (внутри методов если чуть по умному)\n",
    "\n",
    "    # вспомогательные методы ширины и высоты\n",
    "    def width(self):\n",
    "        return abs(self.x2 - self.x1) # abs модуль разности так как ширина неотриц число\n",
    "\n",
    "    def height(self):\n",
    "        return abs(self.y2 - self.y1) # abs модуль разности  так как высота неотриц число\n",
    "\n",
    "    def perimeter(self):\n",
    "        p = 2 * (self.width() + self.height())\n",
    "        return round(p, 2)   # округляем до сотых\n",
    "\n",
    "    def area(self):\n",
    "        s = self.width() * self.height()\n",
    "        return round(s, 2)   # тоже до сотых\n",
    "\n",
    "\n"
   ]
  },
