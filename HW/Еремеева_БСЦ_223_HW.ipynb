   "source": [
    "n = int(input(\"Введите число: \"))\n",
    "\n",
    "result = 0\n",
    "\n",
    "while n != 0:\n",
    "    a = n % 10        # берём последнюю цифру ( остаток от деления на 10 дает посл число)\n",
