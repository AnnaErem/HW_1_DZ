    "    'i': '..',   'j': '.---', 'k': '-.-',  'l': '.-..',\n",
    "    'm': '--',   'n': '-.',   'o': '---',  'p': '.--.',\n",
    "    'q': '--.-', 'r': '.-.',  's': '...',  't': '-',\n",
